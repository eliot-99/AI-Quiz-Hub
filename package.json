{
  "name": "ai-quiz-hub",
  "version": "1.0.0",
  "description": "AI-Powered Quiz Application with elegant UI/UX",
  "main": "index.js",
  "scripts": {
    "dev": "next dev",
    "build": "next build",
    "start": "next start",
    "lint": "next lint",
    "server": "node server/index.js",
    "type-check": "tsc --noEmit",
    "test": "echo \"No tests specified yet\" && exit 0",
    "test:e2e": "echo \"No e2e tests specified yet\" && exit 0",
    "docker:build": "docker build -t ai-quiz-hub .",
    "docker:run": "docker run -p 3000:3000 -p 3001:3001 ai-quiz-hub",
    "docker:compose": "docker-compose up -d",
    "pm2:start": "pm2 start ecosystem.config.js",
    "pm2:stop": "pm2 stop ecosystem.config.js",
    "pm2:restart": "pm2 restart ecosystem.config.js",
    "deploy:vercel": "vercel --prod",
    "deploy:check": "node deploy-check.js",
    "clean": "powershell -NoProfile -Command \"Remove-Item -Recurse -Force .next; if (Test-Path 'node_modules/.cache') { Remove-Item -Recurse -Force 'node_modules/.cache' }\"",
    "postinstall": "echo \"âœ… AI Quiz Hub dependencies installed successfully!\""
  },
  "dependencies": {
    "@google/generative-ai": "^0.2.1",
    "@types/styled-components": "^5.1.34",
    "axios": "^1.6.0",
    "cors": "^2.8.5",
    "dotenv": "^16.3.0",
    "express": "^4.18.0",
    "framer-motion": "^10.16.0",
    "next": "14.2.32",
    "openai": "^4.20.0",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "recharts": "^2.8.0",
    "styled-components": "^6.1.19",
    "tailwindcss": "^3.3.0"
  },
  "devDependencies": {
    "@types/node": "^20.0.0",
    "@types/react": "^18.2.0",
    "@types/react-dom": "^18.2.0",
    "autoprefixer": "^10.4.0",
    "eslint": "^8.0.0",
    "eslint-config-next": "^14.0.0",
    "postcss": "^8.4.0",
    "typescript": "^5.0.0"
  },
  "keywords": [
    "quiz",
    "ai",
    "react",
    "nextjs",
    "education"
  ],
  "author": "Quiz Hub Team",
  "license": "MIT"
}